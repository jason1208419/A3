<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="cs.group11.controllers.SignInController">
    <children>
        <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0" VBox.vgrow="NEVER">
            <children>
                <Label alignment="CENTER" prefHeight="0.0" prefWidth="82.0" text="Sign In">
                    <font>
                        <Font size="27.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <Pane prefHeight="289.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
            <children>
                <TextField id="username" fx:id="usernameTXT" alignment="CENTER" layoutX="205.0" layoutY="81.0"
                           prefHeight="30.0" prefWidth="210.0"/>
                <Button id="signIn" fx:id="signInBtn" alignment="CENTER" layoutX="205.0" layoutY="111.0"
                        mnemonicParsing="false" prefHeight="30.0" prefWidth="210.0" text="Sign In"/>
                <Label alignment="CENTER" layoutX="130.0" layoutY="86.0" text="Username:">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Button id="shutDown" fx:id="shutDownBtn" alignment="CENTER" layoutX="205.0" layoutY="156.0"
                        mnemonicParsing="false" prefHeight="30.0" prefWidth="210.0" text="Shut Down"/>
            </children>
        </Pane>
    </children>
</VBox>
