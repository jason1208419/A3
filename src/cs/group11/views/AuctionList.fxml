<?xml version="1.0" encoding="UTF-8"?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="277.0" prefWidth="628.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="cs.group11.controllers.AuctionController">
    <top>
        <Pane prefHeight="77.0" prefWidth="600.0" BorderPane.alignment="CENTER"/>
    </top>
    <left>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="filterLBL" prefHeight="25.0" prefWidth="63.0" text="Filter type:"/>
                <CheckBox fx:id="sculptBTN" mnemonicParsing="false" prefHeight="38.0" prefWidth="100.0"
                          text="Sculpture"/>
                <CheckBox fx:id="paintBTN" mnemonicParsing="false" prefHeight="33.0" prefWidth="93.0" text="Painting"/>
            </children>
        </VBox>
    </left>
    <right>
        <ScrollBar orientation="VERTICAL" prefHeight="338.0" prefWidth="32.0" BorderPane.alignment="CENTER"/>
    </right>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Label prefHeight="16.0" prefWidth="121.0" text="Auctions"/>
                <ListView fx:id="aucList" prefHeight="200.0" prefWidth="200.0"/>
            </children>
        </VBox>
    </center>
</BorderPane>
