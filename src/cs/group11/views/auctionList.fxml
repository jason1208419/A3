<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="277.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="cs.group11.controllers.AuctionListController">
    <children>
        <HBox prefHeight="74.0" prefWidth="628.0"/>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label fx:id="filterLBL" prefHeight="25.0" prefWidth="63.0" text="Filter type:"/>
                        <CheckBox fx:id="sculptBtn" mnemonicParsing="false" prefHeight="38.0" prefWidth="100.0"
                                  text="Sculpture"/>
                        <CheckBox fx:id="paintBtn" mnemonicParsing="false" prefHeight="33.0" prefWidth="93.0"
                                  text="Painting"/>
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label prefHeight="16.0" prefWidth="121.0" text="Auctions"/>
                        <ListView fx:id="filterAuc" prefHeight="200.0" prefWidth="200.0"/>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</VBox>
